FROM books 
WHERE title LIKE '%the%';</sql><sql name="SQL 10">SELECT title, author, release_year AS 'Release year'
FROM books
WHERE release_year BETWEEN '1960' AND '1990'
ORDER BY 3; </sql><sql name="SQL 11">SELECT students.book_id, title, MIN(review) AS 'Worst rated book', name
FROM books
JOIN students
ON students.book_id=books.book_id;</sql><sql name="SQL 12">SELECT name, penalty AS 'Banned for two months'
FROM students
WHERE penalty='yes'
ORDER BY name;</sql><sql name="SQL 13">SELECT books.book_id, books.title, books.author, COUNT(students.book_id) AS 'Frequency'
FROM books
JOIN students
ON books.book_id=students.book_id
GROUP BY 2
ORDER BY 4 DESC;</sql><sql name="SQL 14">SELECT title, author,
CASE
