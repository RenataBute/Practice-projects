FROM books; </sql><sql name="SQL 4">SELECT MIN(release_year) AS 'Oldest Book', title
FROM books;</sql><sql name="SQL 5">SELECT genre, COUNT(*) AS 'How Many Books'
FROM books
GROUP BY genre
ORDER BY 2 DESC; </sql><sql name="SQL 6">SELECT genre, title, MAX(review) AS 'Highest review in each genre'
FROM books
GROUP BY genre
